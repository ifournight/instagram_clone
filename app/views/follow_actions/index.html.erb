<p id="notice"><%= notice %></p>

<h1>Follow Actions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @follow_actions.each do |follow_action| %>
      <tr>
        <td><%= link_to 'Show', follow_action %></td>
        <td><%= link_to 'Edit', edit_follow_action_path(follow_action) %></td>
        <td><%= link_to 'Destroy', follow_action, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Follow Action', new_follow_action_path %>
