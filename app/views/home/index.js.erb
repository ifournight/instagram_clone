$("#post-list").append("<%= j render(@feeds) %>");

// Update pagination link
<% if @feeds.last_page? %>
  $('.pagination').html("");
<% else %>
  $('.pagination').html("<%=j(link_to_next_page(@feeds, t('app.loadmore'), remote: true, class: 'c-btn c-btn--primary c-btn--small o-wrapper')) %>");
<% end %>